<%- include('include/_header') %>
    <!-- </section>  -->
    <section class="table part" style="width: 100%;height: 700px;border: 1px solid black;">
        
    
   <section class="Add button">
       <div class="row">
           <div class="col-lg-12">
             <a href="/"> <button class="btn btn-outline-info mt-5 ml-5 mb-4" >New user <img src="images/user1.png"></button></a> 
           </div>
       </div>
   </section>
   <section class="table">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="container-fluid">
                    <form action="/viewuser" method="get">
                    <table class="table">
                        <thead class="thead-dark">
                          <tr>
                            <th scope="col">ID</th>
                            <th scope="col">Name</th>
                            <th scope="col">Email</th>
                            <th scope="col">Gender</th>
                            <th scope="col">Action</th>
                          </tr>
                        </thead>
                        <tbody>
                            <%for(var i=0;i<users.length;i++){%>
                            <tr>
                                <td><%=i+1%></td>
                                <td><%=users[i].uname%></td>
                                <td><%=users[i].email%></td>
                                <td><%=users[i].gender%></td>
                                <td><%=users[i].status%> <a href="/deleteuser/<%=users[i]._id%>" class="btn"><i class="fa fa-trash"></i></a> 
                                    <a href="/edituser/<%=users[i]._id%>" class="btn"><i class="fa fa-pencil"></i></a></td>
                            </tr>
                        <%}%>
                        </tbody>
                </div>
            </div>
            
        </div>
    </div>
</table>
</form>
   </section>
</section>
<%- include('include/_footer')%>
